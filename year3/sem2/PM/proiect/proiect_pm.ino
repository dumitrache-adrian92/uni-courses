  display.print(":");
  if (minute < 10)
    display.print(0);
  display.println(minute, DEC);

  display.setTextSize(3);
  display.println(dayOfTheWeek + "\n" + day + " " + month + ", " + year);
}

bool shouldWearJacket(String description, float temperature) {
  if (temperature <= 20.00)
    return true;

  if (description == "shower rain" ||
      description == "rain" ||
      description == "thunderstorm" ||
      description == "snow" ||
      description == "mist")
      return true;

  return false;
}

String checkHumidity(float humidity, float lowerBound, float upperBound, bool checkLowerBound) {
  if (humidity > upperBound)
    return "Humidity level \ntoo high for \noutside \ntemperature";
  else if (checkLowerBound && humidity < lowerBound)
    return "Humidity level \ntoo low for \noutside \ntemperature";
  else
    return "Humidity level \nis ideal";
}

String humidityMessage(float humidity, float temperature) {
  if (temperature >= 10) {
    return checkHumidity(humidity, 0.0, 45.00, false);
  } else if (temperature >= -4 && temperature < 10) {
    return checkHumidity(humidity, 0.0, 40.00, false);
  } else if (temperature >= -17.5 && temperature < -4) {
    return checkHumidity(humidity, 30.00, 40.00, true);
  } else if (temperature >= -29.00 && temperature < -17.5) {
    return checkHumidity(humidity, 20.00, 30.00, true);
  } else if (temperature < -29.00) {
    return checkHumidity(humidity, 15.00, 20.00, true);
  } else
    return "Invalid temperature";
}

