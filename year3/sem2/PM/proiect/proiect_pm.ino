String httpGETRequest(const char* serverName) {
  WiFiClient client;
  HTTPClient http;

  // Your IP address with path or Domain name with URL path
  http.begin(client, serverName);

  // Send HTTP POST request
  int httpResponseCode = http.GET();

  String payload = "{}";

  if (httpResponseCode > 0) {
    Serial.print("HTTP Response code: ");
    Serial.println(httpResponseCode);
    payload = http.getString();
  }
  else {
    Serial.print("Error code: ");
    Serial.println(httpResponseCode);
  }
  // Free resources
  http.end();

  return payload;
}

void getClockInfo() {
  year = 2000 + RTC.getYear();
  month = getMonthName(RTC.getMonth(century));
  day = RTC.getDate();
  dayOfTheWeekNumeric = RTC.getDoW();
  dayOfTheWeek = getDayOfTheWeek(RTC.getDoW());
  hour = RTC.getHour(h12, hPM);
  minute = RTC.getMinute();
}

void updateClock() {
  year = MyClock.Year();
  month = getMonthName(MyClock.Month());
  if (day < MyClock.Day()) {
    day = MyClock.Day();
    dayOfTheWeekNumeric = dayOfTheWeekNumeric + 1;
    if (dayOfTheWeekNumeric == 8)
      dayOfTheWeekNumeric = 1;
    dayOfTheWeek = getDayOfTheWeek(dayOfTheWeekNumeric);
  }
  hour = MyClock.Hour();
  minute = MyClock.Minute();
}

