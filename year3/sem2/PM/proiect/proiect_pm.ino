typedef enum {
  weather = 0,
  humidity = 1,
  clock_and_date = 2,
  max_state = 3
} displayState;

volatile displayState state = weather;
float temperatureVal = 0.0;
float humidityVal = 0.0;
displayState lastState = max_state;

unsigned long lastWeatherReset = 0;
unsigned long lastClockReset = 0;

void setup() {
  Wire.begin();
  Serial.begin(57600);

  // set up clock
  getClockInfo();
  DateTime currentMoment = RTClib::now();

  MyClock.SetClock(currentMoment.unixtime(), false);

  // display setup
  display.begin();
  display.fillScreen(ILI9341_BLACK);
  display.setRotation(1);
  display.drawBitmap(0, 0, pmLogo, 320, 240, 0x83F8); //ILI9341_WHITE);

  // setup WiFi connection
  WiFiManager wm;

  // reset settings - wipe stored credentials for testing
  // these are stored by the esp library
  // wm.resetSettings();

  // Automatically connect using saved credentials,
  // if connection fails, it starts an access point with the specified name ( "AutoConnectAP"),
  // if empty will auto generate SSID, if password is blank it will be anonymous AP (wm.autoConnect())
  // then goes into a blocking loop awaiting configuration and will return success result
  bool res;
  res = wm.autoConnect("SmartMirrorAP","password"); // password protected ap

  if(!res) {
    Serial.println("Failed to connect");
  }
  else {
    //if you get here you have connected to the WiFi
    Serial.println("Connected to WiFi!");
  }

  // button setup
  pinMode(BUTTON_PIN, INPUT_PULLUP);
  attachInterrupt(digitalPinToInterrupt(BUTTON_PIN), button_isr, FALLING);

  // get IP
  getIPInfo();
  // get location based on IP
  getLocationInfo();
  // get
  getWeatherInfo();

  // DHT setup
  delay(2000);
  dht22.read2(&temperatureVal, &humidityVal, NULL);

  display.fillScreen(ILI9341_BLACK);
}

void loop() {
  // update weather and humidity information every 10 minutes
  if (millis() - lastWeatherReset >= 600000) {
    lastWeatherReset = millis();

    getWeatherInfo();
    humidityVal = 0.0;

    if (state == weather || state == humidity)
      lastState = max_state;
  }

	// check if clock needs to be updated
  Clock::EEvent eEvent;
	eEvent = MyClock.CheckEvent();

  if (eEvent != Clock::eEventNone) {
		updateClock();

    if (state == clock_and_date)
      lastState = max_state;
	}

  // if we haven't already gotten a humidity reading, keep trying
  if (state == humidity && humidityVal - 0.00 <= 0.001) {
    int err = dht22.read2(&temperatureVal, &humidityVal, NULL);

    if (err != SimpleDHTErrSuccess) {
      Serial.println("error");
    } else {
      lastState = max_state;
    }
  }

  if (state == lastState)
    return;

  // reset screen and draw screen according to state
  display.fillScreen(ILI9341_BLACK);

  if (state == weather) {
    weatherScreen();
  } else if (state == humidity) {
    humidityScreen();
  } else if (state == clock_and_date) {
    clockScreen();
  }

  Serial.println(state);

  lastState = state;

  delay(150);
}

ICACHE_RAM_ATTR void button_isr() {
  if(millis() - lastButtonInterrupt > 250) {
    state = static_cast<displayState>(static_cast<int>(state) + 1);

    if (state == max_state)
      state = weather;

    lastButtonInterrupt = millis();
  }
}

