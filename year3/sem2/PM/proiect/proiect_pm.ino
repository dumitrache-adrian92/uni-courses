String normalizeString(String input) {
  // Arrays to map Romanian diacritics to their simpler versions
  const char diacritics[] = "ĂÂÎȘȚăâîșț";
  const char replacements[] = "AAISTaaisst";

  // Initialize the output string
  String output = input;

  // Iterate over each character in the input string
  for (size_t i = 0; i < output.length(); ++i) {
      if (output[i] == 197) {
        output[i] = 's';
        for (int j = i + 1; j < output.length() - 1; j++) {
          output[j] = output[j + 1];
        }

        output[output.length() - 1] = '\0';
      }
  }

  return output;
}

String getLastWord(const String &input) {
    // Trim any trailing spaces
    String trimmedInput = input;
    trimmedInput.trim();

    // Find the position of the last space
    int lastSpaceIndex = trimmedInput.lastIndexOf(' ');

    // Extract and return the last word
    if (lastSpaceIndex == -1) {
        // No spaces found, the entire input is one word
        return trimmedInput;
    } else {
        // Return the substring after the last space
        return trimmedInput.substring(lastSpaceIndex + 1);
    }
}
